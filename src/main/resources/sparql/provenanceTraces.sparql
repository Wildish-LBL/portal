PREFIX wfprov: <http://purl.org/wf4ever/wfprov#>
PREFIX ro: <http://purl.org/wf4ever/ro#>
PREFIX ao: <http://purl.org/ao/>

SELECT ?resource ?trace 
WHERE { 
<%s> <http://www.openarchives.org/ore/terms/aggregates> ?ann .
?ann a wfprov:ProvenanceAnnotation ;
ro:annotatesAggregatedResource ?resource ;
ao:body ?trace .
}
